# 练习
library(CLL)
data("sCLLex")
View(sCLLex)
sCLLex
exprMatrix = exprs(sCLLex)
dim(exprMatrix)
meta = pData(sCLLex)
table(meta$Disease)
View(sCLLex)
## limma使用ExpressionSet的数据作为输入数据的练习
library(limma)
design = model.matrix(~factor(sCLLex$Disease))
View(design)
fit = lmFit(sCLLex,design)
View(fit)
fit = eBayes(fit)
options(digits = 4)
topTable(fit,coef = 2,adjust = 'BH')
